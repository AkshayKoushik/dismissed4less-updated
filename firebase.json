{
  "hosting": {
    "public": "__sapper__/export",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=172800"
          }
        ]
      }
    ]
  }
}
